{
  "name": "remote-desktop-web-client",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win portable",
    "build-linux": "electron-builder --linux appimage",
    "build-macos": "electron-builder --mac dmg"
  },
  "devDependencies": {
    "electron": "^35.1.2",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.example.guacamolebrowser",
    "productName": "remote-desktop-web-client",
    "win": {
      "target": "portable",
      "asar": true,
      "icon": "assets/icon.ico"
    },
    "files": [
      "main.js",
      "assets/icon.png",
      "splash.html"
    ],
    "extraMetadata": {
      "main": "main.js"
    }
  },
  "build-linux": {
    "appId": "com.example.guacamolebrowser",
    "productName": "remote-desktop-web-client",
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    },
    "files": [
      "main.js",
      "assets/icon.png",
      "splash.html"
    ],
    "extraMetadata": {
      "main": "main.js"
    }
  },
  "build-mac": {
    "appId": "com.example.guacamolebrowser",
    "productName": "remote-desktop-web-client",
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "files": [
      "main.js",
      "assets/icon.icns",
      "splash.html"
    ],
    "extraMetadata": {
      "main": "main.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
